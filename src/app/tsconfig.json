{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "lib": [
      "es6",
      "dom"
    ],
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true
  },
  "typedocOptions": {
    "mode": "modules",
    "out": "docs",
    "exclude": [
      "**/src/__tests__/**"
    ]
  },
  "include": [
    "src"
  ]
}
